{
  "name": "@lanonasis/enterprise-context",
  "version": "1.0.0",
  "description": "Enterprise Context Memory Service with API Key Management and MCP Integration",
  "main": "dist/server.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "bun run type-check && tsc && tsc-alias",
    "start": "node dist/index.js",
    "vercel-build": "bun run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t lanonasis-maas .",
    "docker:run": "docker run -p 3000:3000 lanonasis-maas",
    "docker:push": "docker push lanonasis-maas",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "deploy": "./scripts/deploy.sh deploy",
    "deploy:staging": "./scripts/deploy.sh deploy --env staging",
    "deploy:production": "./scripts/deploy.sh deploy --env production",
    "publish:all": "./scripts/deploy.sh publish-sdk && ./scripts/deploy.sh publish-cli",
    "publish:sdk": "./scripts/deploy.sh publish-sdk",
    "publish:cli": "./scripts/deploy.sh publish-cli",
    "build:all": "./scripts/deploy.sh build",
    "clean": "./scripts/deploy.sh clean",
    "workspace:install": "npm ci && npm ci --prefix cli && npm ci --prefix packages/lanonasis-sdk",
    "workspace:build": "npm run build && npm run build --prefix cli && npm run build --prefix packages/lanonasis-sdk",
    "workspace:test": "npm run test && npm run test --prefix packages/lanonasis-sdk",
    "workspace:lint": "npm run lint && npm run lint --prefix cli && npm run lint --prefix packages/lanonasis-sdk"
  },
  "keywords": [
    "memory",
    "ai",
    "microservice",
    "vector-search",
    "enterprise",
    "api-keys",
    "secrets",
    "mcp",
    "security",
    "maas"
  ],
  "author": "Seye Derick",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.76.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vercel/node": "^3.0.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
