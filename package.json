{
  "name": "@lanonasis/enterprise-context",
  "version": "1.0.0",
  "description": "Enterprise Context Memory Service with API Key Management and MCP Integration",
  "main": "dist/server.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "vercel-build": "bun run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t lanonasis-maas .",
    "docker:run": "docker run -p 3000:3000 lanonasis-maas",
    "docker:push": "docker push lanonasis-maas",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "deploy": "./scripts/deploy.sh deploy",
    "deploy:staging": "./scripts/deploy.sh deploy --env staging",
    "deploy:production": "./scripts/deploy.sh deploy --env production",
    "publish:all": "./scripts/deploy.sh publish-sdk && ./scripts/deploy.sh publish-cli",
    "publish:sdk": "./scripts/deploy.sh publish-sdk",
    "publish:cli": "./scripts/deploy.sh publish-cli",
    "build:all": "./scripts/deploy.sh build",
    "clean": "./scripts/deploy.sh clean",
    "workspace:install": "npm ci && npm ci --prefix cli && npm ci --prefix packages/lanonasis-sdk",
    "workspace:build": "npm run build && npm run build --prefix cli && npm run build --prefix packages/lanonasis-sdk",
    "workspace:test": "npm run test && npm run test --prefix packages/lanonasis-sdk",
    "workspace:lint": "npm run lint && npm run lint --prefix cli && npm run lint --prefix packages/lanonasis-sdk"
  },
  "keywords": [
    "memory",
    "ai",
    "microservice",
    "vector-search",
    "enterprise",
    "api-keys",
    "secrets",
    "mcp",
    "security",
    "maas"
  ],
  "author": "Seye Derick",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.79.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "next": "16.0.10",
    "openai": "^6.8.1",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "redis": "^5.9.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwindcss": "^4.1.16",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vercel/node": "^5.5.4",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
